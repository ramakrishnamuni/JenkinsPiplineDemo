pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This is build stage - TESTING AUTOMATION BUILD -1'
            }
        }
         stage('Test') {
            steps {
                echo 'This is test stage'
            }
        }
         stage('Deploy') {
            steps {
                echo 'This is deploy stage'
            }
        }
        stage('Relase') {
            steps {
                echo 'This is release stage'
            }
        }
    }
}
